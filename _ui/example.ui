<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="ttk.Panedwindow" id="panedwindow">
    <property name="height">600</property>
    <property name="orient">vertical</property>
    <property name="width">900</property>
    <layout manager="pack">
      <property name="expand">true</property>
      <property name="fill">both</property>
      <property name="padx">5</property>
      <property name="pady">5</property>
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Panedwindow.Pane" id="pane_top">
        <property name="weight">1</property>
        <child>
          <object class="ttk.Frame" id="frame_buttons">
            <property name="borderwidth">2</property>
            <property name="height">50</property>
            <property name="relief">groove</property>
            <layout manager="pack">
              <property name="anchor">ne</property>
              <property name="expand">false</property>
              <property name="propagate">True</property>
              <property name="side">right</property>
            </layout>
            <child>
              <object class="ttk.Frame" id="frame_scalepercent">
                <property name="borderwidth">2</property>
                <property name="padding">2</property>
                <property name="relief">groove</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="expand">false</property>
                  <property name="propagate">True</property>
                  <property name="side">right</property>
                </layout>
                <child>
                  <object class="ttk.Label" id="lb_T1">
                    <property name="text" translatable="yes">Label T1</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lb_scalevar">
                    <property name="font">{Arial} 10 {bold}</property>
                    <property name="text" translatable="yes">100</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Scale" id="scale_1_2">
                    <property name="orient">horizontal</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame_orig_or_adjusted">
                <property name="borderwidth">2</property>
                <property name="padding">2</property>
                <property name="relief">flat</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="propagate">True</property>
                  <property name="side">right</property>
                </layout>
                <child>
                  <object class="tk.Checkbutton" id="cb_2">
                    <property name="cursor">hand2</property>
                    <property name="text" translatable="yes">Checkbox2</property>
                    <layout manager="pack">
                      <property name="padx">5</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame_cb_opencv">
                <property name="padding">2</property>
                <property name="relief">flat</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="padx">5</property>
                  <property name="propagate">True</property>
                  <property name="side">left</property>
                </layout>
                <child>
                  <object class="tk.Checkbutton" id="cb_1">
                    <property name="cursor">hand2</property>
                    <property name="text" translatable="yes">Checkbox1</property>
                    <layout manager="pack">
                      <property name="padx">5</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Panedwindow.Pane" id="pane_body">
        <property name="weight">16</property>
        <child>
          <object class="ttk.Frame" id="frame_body">
            <property name="borderwidth">3</property>
            <property name="height">200</property>
            <property name="relief">flat</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="ttk.Panedwindow" id="panedwindow_body">
                <property name="orient">horizontal</property>
                <layout manager="pack">
                  <property name="expand">true</property>
                  <property name="fill">both</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Panedwindow.Pane" id="pane_left">
                    <property name="weight">1</property>
                    <child>
                      <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_left">
                        <property name="scrolltype">both</property>
                        <property name="usemousewheel">false</property>
                        <layout manager="pack">
                          <property name="expand">true</property>
                          <property name="fill">both</property>
                          <property name="propagate">True</property>
                          <property name="side">top</property>
                        </layout>
                        <child>
                          <object class="ttk.Panedwindow" id="panedwindow_left">
                            <property name="height">200</property>
                            <property name="orient">vertical</property>
                            <property name="width">200</property>
                            <layout manager="pack">
                              <property name="expand">true</property>
                              <property name="fill">both</property>
                              <property name="propagate">True</property>
                              <property name="side">top</property>
                            </layout>
                            <child>
                              <object class="ttk.Panedwindow.Pane" id="pane_left_first">
                                <property name="weight">1</property>
                                <child>
                                  <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_left_first">
                                    <property name="relief">groove</property>
                                    <property name="scrolltype">both</property>
                                    <property name="usemousewheel">true</property>
                                    <layout manager="pack">
                                      <property name="anchor">nw</property>
                                      <property name="expand">false</property>
                                      <property name="ipadx">100</property>
                                      <property name="ipady">60</property>
                                      <property name="padx">10</property>
                                      <property name="pady">10</property>
                                      <property name="propagate">True</property>
                                      <property name="side">left</property>
                                    </layout>
                                    <child>
                                      <object class="ttk.Frame" id="frame_L1">
                                        <property name="height">200</property>
                                        <property name="padding">2</property>
                                        <property name="relief">flat</property>
                                        <property name="width">200</property>
                                        <layout manager="pack">
                                          <property name="anchor">nw</property>
                                          <property name="expand">false</property>
                                          <property name="fill">x</property>
                                          <property name="padx">5</property>
                                          <property name="pady">10</property>
                                          <property name="propagate">True</property>
                                          <property name="side">top</property>
                                        </layout>
                                        <child>
                                          <object class="ttk.Label" id="lb_1">
                                            <property name="text" translatable="yes">Label1</property>
                                            <layout manager="pack">
                                              <property name="propagate">True</property>
                                              <property name="side">left</property>
                                            </layout>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="ttk.Label" id="lb_1val">
                                            <property name="text" translatable="yes">0</property>
                                            <layout manager="pack">
                                              <property name="propagate">True</property>
                                              <property name="side">right</property>
                                            </layout>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="ttk.Scale" id="scale_1">
                                            <property name="cursor">hand2</property>
                                            <property name="from_">0</property>
                                            <property name="orient">horizontal</property>
                                            <property name="to">255</property>
                                            <bind sequence="&lt;B1-Motion&gt;" handler="get_scale1val" add="" />
                                            <layout manager="pack">
                                              <property name="expand">true</property>
                                              <property name="fill">x</property>
                                              <property name="padx">5</property>
                                              <property name="propagate">True</property>
                                              <property name="side">left</property>
                                            </layout>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="ttk.Panedwindow.Pane" id="pane_left_second">
                                <property name="weight">4</property>
                                <child>
                                  <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_left_second">
                                    <property name="relief">groove</property>
                                    <property name="scrolltype">both</property>
                                    <property name="usemousewheel">false</property>
                                    <layout manager="pack">
                                      <property name="propagate">True</property>
                                      <property name="side">top</property>
                                    </layout>
                                    <child>
                                      <object class="ttk.Frame" id="frame_L2">
                                        <property name="height">200</property>
                                        <property name="width">200</property>
                                        <layout manager="pack">
                                          <property name="fill">both</property>
                                          <property name="propagate">True</property>
                                          <property name="side">top</property>
                                        </layout>
                                        <child>
                                          <object class="ttk.Label" id="lb_2">
                                            <property name="text" translatable="yes">Label2</property>
                                            <layout manager="pack">
                                              <property name="padx">10</property>
                                              <property name="propagate">True</property>
                                              <property name="side">left</property>
                                            </layout>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="ttk.Combobox" id="combobox_1">
                                            <property name="cursor">hand2</property>
                                            <property name="state">readonly</property>
                                            <property name="width">10</property>
                                            <bind sequence="&lt;&lt;ComboboxSelected&gt;&gt;" handler="get_cb2" add="" />
                                            <layout manager="pack">
                                              <property name="expand">false</property>
                                              <property name="propagate">True</property>
                                              <property name="side">left</property>
                                            </layout>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="ttk.Panedwindow.Pane" id="pane_right">
                    <property name="weight">5</property>
                    <child>
                      <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_right">
                        <property name="scrolltype">both</property>
                        <property name="usemousewheel">true</property>
                        <layout manager="pack">
                          <property name="expand">true</property>
                          <property name="fill">both</property>
                          <property name="padx">20</property>
                          <property name="pady">10</property>
                          <property name="propagate">True</property>
                          <property name="side">left</property>
                        </layout>
                        <child>
                          <object class="tk.Canvas" id="canvas_1">
                            <property name="background">#ffffff</property>
                            <property name="confine">false</property>
                            <property name="height">1200</property>
                            <property name="highlightbackground">#ffffff</property>
                            <property name="width">1920</property>
                            <layout manager="pack">
                              <property name="anchor">center</property>
                              <property name="expand">true</property>
                              <property name="fill">both</property>
                              <property name="propagate">True</property>
                              <property name="side">top</property>
                            </layout>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Panedwindow.Pane" id="pane_bottom">
        <property name="weight">2</property>
        <child>
          <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe_5">
            <property name="borderwidth">2</property>
            <property name="relief">raised</property>
            <property name="scrolltype">both</property>
            <property name="usemousewheel">false</property>
            <layout manager="pack">
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="tk.Text" id="text_1">
                <property name="height">10</property>
                <property name="text" translatable="yes">text_1</property>
                <property name="width">50</property>
                <layout manager="pack">
                  <property name="fill">both</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
